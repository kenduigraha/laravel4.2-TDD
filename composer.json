{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
	"framework",
	"laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
	"laravel/framework": "4.2.*",
	"laravelbook/ardent": "2.*",
	"mathiasverraes/money": "1.2.1"
  },

  "require-dev": {
	"phpunit/phpunit": "4.3.*",
	"way/generators": "~2.0",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
	"classmap": [
	  "app/commands",
	  "app/controllers",
	  "app/models",
      "app/library",
	  "app/database/migrations",
	  "app/database/seeds",
	  "app/tests/TestCase.php"
	],
    "psr-0": {
        "Tesis\\": "app/library/"
    }
  },
  "autoload-dev": {
		"classmap":[
            "app/tests/helpers"
        ]
	},
  "scripts": {
	"post-install-cmd": [
	  "php artisan clear-compiled",
	  "php artisan optimize"
	],
	"post-update-cmd": [
	  "php artisan clear-compiled",
	  "php artisan optimize"
	],
	"post-create-project-cmd": [
	  "php artisan key:generate"
	]
  },
  "config": {
	"preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
